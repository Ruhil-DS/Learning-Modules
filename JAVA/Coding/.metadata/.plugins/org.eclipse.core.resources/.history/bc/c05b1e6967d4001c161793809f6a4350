class Employee{
	    String ename;
	    String eid;
	    String edept;
	    
	    public Employee(){
		        ename = "guest";
	    }
//Define the required methods
	    
	    public Employee(String name, String id, String dept) {
	    	this.ename = name;
	    	this.eid = id;
	    	this.edept = dept;
	    }
	    
	    public void copyDept(Employee obj) {
	    	this.edept = obj.edept;
	    }
	    
    	public void displayDetails() {
    		System.out.println("ename : " + this.ename);
    		System.out.println("eid : " + this.eid);
    		System.out.println("edept : " + this.edept);
    	}
    	
//Define the required methods
	    
}